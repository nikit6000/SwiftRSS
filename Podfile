# Uncomment the next line to define a global platform for your project
platform :ios, '8.0'

target 'SwiftRSS' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  pod 'Alamofire'  
  pod 'IGListKit', '~> 3.4.0'
  pod 'SwiftyXML', '~> 3.0.0'
  pod 'SDWebImage', '~> 5.0'

  # Pods for SwiftRSS

  target 'SwiftRSSTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SwiftRSSUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
